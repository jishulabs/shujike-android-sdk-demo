<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="demo.sdk.android.com.shujike.jcenter.shujike_android_sdk_demo">

    <application
        android:name=".MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon, android:theme,android:label,android:allowBackup">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <!--集成相关-->
            <intent-filter>
                <data android:scheme="sjk.3020df199745feec" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>

        </activity>
        <!--集成相关-->
        <meta-data
            android:name="SHUJIKE_APPKEY"
            android:value="3020df199745feec" />
        <meta-data
            android:name="SHUJIKE_CHANNEL"
            android:value="yingyongbao" />
    </application>
</manifest>